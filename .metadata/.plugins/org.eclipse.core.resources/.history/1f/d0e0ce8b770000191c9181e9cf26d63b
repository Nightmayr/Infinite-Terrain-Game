

public class PoI {
	
	private int xCoord;
	private int yCoord;
	

	public void setLocation() {
		xCoord = 1;
		yCoord = 1;
		
	}

	public int getxCoord() {
		return xCoord;
	}

	public void setxCoord(int xCoord) {
		this.xCoord = xCoord;
	}

	public int getyCoord() {
		return yCoord;
	}

	public void setyCoord(int yCoord) {
		this.yCoord = yCoord;
	}
	
	
	
}
