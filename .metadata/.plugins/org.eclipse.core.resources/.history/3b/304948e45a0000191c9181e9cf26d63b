import java.util.Scanner;

public class Player {

	String input;
	int xCoord;
	int yCoord;

	public void Start() {
		System.out.println();
		System.out.println(input);
		Scanner sc = new Scanner(System.in);
		input = sc.next();
		System.out.println(input);

	}

	public void playerChoice() {
		if (input.equals("n")) {
			yCoord += 1;
		} else if (input.equals("s")) {
			yCoord -= 1;
		} else if (input.equals("e")) {
			xCoord += 1;
		}else if (input.equals("w")){
			xCoord -= 1;
		}
	}

}
